<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 配置哪些包下的类全名可以简写 -->
    <typeAliases>
        <package name="com.xingyu.model"/>
    </typeAliases>
    <!-- 配置要读取哪些xxxMapper.xml文件的位置 -->
    <mappers>
        <mapper resource="mybatis/mappers/GuestMapper.xml"/>
    </mappers>
</configuration>
